{
  "name": "senars",
  "version": "10.0.0",
  "description": "SeNARS (Semantic Non-Axiomatic Reasoning System)",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "repl:agent": "node src/repl/AgentRepl.js",
    "repl:agent:ollama": "node src/repl/AgentRepl.js --ollama gemma3:4b",
    "start": "node scripts/cli/run.js",
    "cli": "node scripts/cli/run.js",
    "dev": "node scripts/cli/run.js --dev",
    "web": "node scripts/ui/launcher.js",
    "web:dev": "node scripts/ui/launcher.js",
    "web:prod": "node scripts/ui/launcher.js",
    "web:graph": "node scripts/ui/launcher.js --graph-ui",
    "repl": "node scripts/ui/launcher.js --repl",
    "repl:tui": "node -e \"import('./src/repl/TUIReplInk.js').then(m => new m.TUIReplInk().start())\"",
    "repl:test": "node -e \"import('./src/repl/ReplEngine.js').then(async m => { const engine = new m.ReplEngine(); await engine.initialize(); console.log('âœ… ReplEngine works'); await engine.shutdown(); })\"",
    "demo:tui": "node -e \"import('./src/repl/tui-demo.js').then(m => m.runTUIDemo())\"",
    "demo": "node scripts/utils/run-demo.js",
    "screenshots": "node scripts/utils/visualize.js --type screenshots",
    "movies": "node scripts/utils/visualize.js --type movie",
    "capture": "node scripts/utils/visualize.js",
    "capture:priority": "node scripts/utils/visualize.js --type priority",
    "capture:derivations": "node scripts/utils/visualize.js --type derivations",
    "analyze": "node self-analyze.js",
    "rule-analysis": "node scripts/utils/deep_rule_analysis.js",
    "test": "npm run test:core",
    "test:all": "npm run test:core && npm run test:ui",
    "test:core": "NODE_NO_WARNINGS=1 NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test:ui": "cd ui && npm test",
    "test:unit": "NODE_NO_WARNINGS=1 NODE_OPTIONS=--experimental-vm-modules npx jest tests/unit",
    "test:integration": "NODE_NO_WARNINGS=1 NODE_OPTIONS=--experimental-vm-modules npx jest tests/integration",
    "test:debug": "NODE_NO_WARNINGS=1 NODE_OPTIONS=--experimental-vm-modules npx jest --detectOpenHandles tests/reason/Metacognition.integration.test.js",
    "test:property": "NODE_NO_WARNINGS=1 NODE_OPTIONS=--experimental-vm-modules npx jest tests/nal/propertyBasedTests.test.js",
    "test:automated": "node scripts/utils/test-framework.js",
    "test:e2e": "cd ui && npm run test:e2e",
    "test:ui-screenshots": "cd ui && npm run test:screenshots",
    "config:compare": "node scripts/utils/config-comparison.js",
    "data:export": "node scripts/utils/data-management.js --export",
    "data:import": "node scripts/utils/data-management.js --import",
    "data:backup": "node scripts/utils/data-management.js --backup",
    "data:restore": "node scripts/utils/data-management.js --restore",
    "data:clean": "node scripts/utils/data-management.js --clean",
    "ai:develop": "node scripts/utils/autonomous-development.js",
    "ai:tune-heuristics": "node scripts/utils/autonomous-development.js --mode tune-heuristics",
    "ai:ui-optimize": "node scripts/utils/autonomous-development.js --mode ui-optimization",
    "perf:monitor": "node scripts/utils/performance-monitor.js",
    "perf:profile": "node scripts/utils/performance-monitor.js --profile",
    "perf:benchmark": "node scripts/utils/performance-monitor.js --benchmark",
    "dev:workflow": "node scripts/utils/automated-dev-workflow.js",
    "dev:visual-inspection": "node scripts/utils/automated-dev-workflow.js --visual-inspection",
    "dev:tune-heuristics": "node scripts/utils/automated-dev-workflow.js --tune-heuristics",
    "dev:regression": "node scripts/utils/automated-dev-workflow.js --regression-tests",
    "build": "npm run build:parser",
    "build:parser": "npx peggy --format es -o src/parser/peggy-parser.js src/parser/narsese.peggy",
    "benchmark": "node src/testing/runBenchmarks.js",
    "slides:dev": "slidev slides.md",
    "slides:pdf": "slidev export slides.md"
  },
  "keywords": [
    "senars",
    "agi",
    "nars",
    "cognitive-architecture",
    "neuro-symbolic",
    "reasoning"
  ],
  "author": "Developers",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "@langchain/core": "^1.0.2",
    "@langchain/ollama": "^1.0.0",
    "@langchain/openai": "^1.0.0",
    "@modelcontextprotocol/sdk": "^1.21.1",
    "@xenova/transformers": "^2.17.2",
    "blessed": "^0.1.81",
    "danfojs": "^1.2.0",
    "eventemitter3": "^5.0.1",
    "fast-check": "^3.19.0",
    "glob": "^10.4.5",
    "immer": "^10.0.4",
    "ink": "^6.4.0",
    "ink-big-text": "^2.0.0",
    "ink-gradient": "^3.0.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "js-sha256": "^0.11.1",
    "langchain": "^1.0.2",
    "mitt": "^3.0.1",
    "node-fetch": "^3.3.2",
    "puppeteer": "^24.27.0",
    "react": "^19.1.1",
    "slidev-theme-dracula": "^0.2.0",
    "uuid": "^9.0.1",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.28.3",
    "@slidev/cli": "^52.6.0",
    "@slidev/theme-default": "^0.25.0",
    "babel-jest": "^30.2.0",
    "jest": "^30.2.0",
    "peggy": "^5.0.6",
    "playwright-chromium": "^1.56.1",
    "vitest": "^4.0.8"
  }
}
